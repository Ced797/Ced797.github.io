  width: 99vw;
  text-align: center;
}
#homepage #filter {
  width: 86vw;
  height: 100vh;
  margin-left: 4vw;
  margin-bottom: 10vh;
  margin-top: 10vh;
  background-color: #D9D6CB;
  border-radius: 10px;
  padding: 2vw;
}
#homepage #filter h2 {
  color: #0F1426;
  width: 80vw;
  text-align: center;
}
#homepage #filter input {
  border-radius: 10px;
  margin-bottom: 1vw;
  padding: 1%;
  padding-right: 1vw;
  outline: none;
  font-size: 35px;
  font-family: "NimitzRough", Serif;
  width: 80vw;
  border: 3px solid #0F1426;
  background-color: #D9D6CB;
  color: #0F1426;
}
#homepage #filter li {
  width: 25vw;
  height: 35vh;
  border-radius: 10px;
  float: left;
  margin-right: 2vw;
  margin-bottom: 3vh;
  background-size: 25vw 35vh;
}
#homepage #filter div {
  background-color: rgba(15, 20, 38, 0.7);
  padding-left: 1vw;
  position: relative;
  top: 31vh;
  border-radius: 0px 0px 10px 10px;
}
#homepage #filter li > a {
  color: #D9D6CB;
  font-size: 45px;
}
#homepage p {
  font-family: "NimitzRough", Serif;
  color: #0F1426;
  font-size: 60px;
}
#homepage .articleList ul img {
  width: 500px;
}
#homepage footer {
  background-image: url("/images/headerImage.jpg");
  padding-left: 2vw;
  padding-top: 2vh;
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  grid-template-rows: repeat(auto, 1fr);
}
#homepage footer #disclaimer {
  font-size: 20px;
}
#homepage footer #footerContent {
  grid-row: 1/3;
  grid-column: 1/1;
}
#homepage footer #footerContent div {
  margin-top: 2vh;
}
#homepage footer p {
  font-family: "NimitzRough", Serif;
  color: #D9D6CB;
  font-size: 40px;
  width: 40vw;
}
#homepage footer p + p {
  grid-column: 2/2;
  grid-row: 2/2;
}

